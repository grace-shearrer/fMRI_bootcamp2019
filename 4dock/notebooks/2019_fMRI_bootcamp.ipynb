    "def prepro(basedir):\n",
    "    for item in glob.glob(os.path.join(basedir,'sub-*','func','sub-*.nii.gz')):\n",
    "        input=item\n",
    "        output_path=item.strip('.nii.gz')\n",
    "        output=output_path+('_brain')\n",
